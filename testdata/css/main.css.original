body{
	font-family:"Trebuchet MS","Bitstream Vera Sans","Lucida Sans",sans-serif;
	font-size:15px;
	color:#000;
}

#content{
	position:absolute;
	border:none;
	background-color:#fff;
	top:0px;
	left:0px;
	right:0px;
	margin-bottom:15px;
	
}

#menu{
	position:fixed;
	border-bottom:2px solid green;
	border-left:2px solid green;
	border-right:2px solid green;
	margin-right:12px;
	float:right;
	//left:0px;
	top:0px;
	right:0px;
	z-index:10000;
	background-color:#d9ffd5;
	opacity:0.9;
}

#menu a{
	font-size:12px;
	display:block;
	float:left;
	//width:140px;
	text-decoration:none;
	color:green;
	font-weight:bold;
	margin:2px;
	padding:3px;
	border:1px solid silver;
	background-color:#fff;
	text-align:center;
}

#menu a:hover{
	color:#fff;
	background-color:green;
}

table{
	border-collapse:collapse;
	border:2px solid silver;
	margin-top:12px;
	margin-bottom:12px;
	margin-left:12px;
	margin-right:12px;
}

th{
	font-weight:bold;
}

td{
	margin:0px;
	padding:0px;
	border:1px solid silver;
}

h1{
	font-size:27px;
	font-weight:bold;
	color:green;
	margin:12px;
	margin-bottom:12px;
}

h2{
	font-size:20px;
	font-weight:bold;
	color:green;
	margin:12px;
	margin-bottom:12px;
}

p{
	
	margin:12px;
	
}

.floatLeft{
	display:block;
	float:left;
	margin:12px;
	padding:0px;
	border:none;
}

.floatRight{
	display:block;
	float:right;
	margin:12px;
	padding:0px;
	border:none;
}

a{
	text-decoration:none;
	color:green;
	font-weight:bold;
	font-size:16px;
}

a:hover{
	background-color:green;
	color:white;
}

b{
	font-weight:bold;
	font-size:16px;
	color:green;
}

.fail{
	color:red;
}

.header{
	background-color:green;
	color:white;
	font-weight:bold;
	font-size:25px;
	padding-left:12px;
}

.clearFloats{
	clear:both;
}

code{
	font-family:"Bitstream Vera Sans Mono",monospace;
	color:green;
}

.code{
	font-family:"Bitstream Vera Sans Mono",monospace;
	color:green;
}

.NotaBene{
	color:red;
	font-weight:bold;
}

.quotation{
	margin-left:25px;
	margin-right:25px;
	font-style:italic;
}


